extern "printf";
extern "exit";

fn main() {
    let count: u32 = 100;

    let n: u32 = 2;
    while n <= count {

        let p: u32 = 2;
        while p < n {

            if n % p == 0 {
                p = count + 1;
            }

            p = p + 1;
        }

        if p == n {
            printf("%d\n", n);
        }

        n = n + 1;
    }

    exit(0);
}
