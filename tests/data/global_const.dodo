extern "printf";
extern "exit";

const greeting: u8* = "Hello world!";

const number: u32 = 4321;

const boolean1: bool = false;
const boolean2: bool = true;

fn main() {
    printf("%s\n", greeting);
    printf("The number is %u\n", number);
    printf("Boolean one is %hhu\n", boolean1);
    printf("Boolean two is %hhu\n", boolean2);

    exit(0);
}
